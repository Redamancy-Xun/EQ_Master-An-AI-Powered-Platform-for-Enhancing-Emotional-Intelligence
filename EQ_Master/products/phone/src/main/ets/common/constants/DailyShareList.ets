// 根据id寻找对应的DailyShareItem
export function findDailyShareItemById(id: string) : DailyShareItem {
  let title = id.substring(0, id.indexOf('-'));
  let dailyShareItemList: DailyShareItem[] = [];
  if (title == 'question') {
    dailyShareItemList = DAILY_SHARE_LIST_QUESTION;
  } else if (title == 'tip') {
    dailyShareItemList = DAILY_SHARE_LIST_TIP;
  } else {
    dailyShareItemList = DAILY_SHARE_LIST_STORY;
  }
  for (let i = 0; i < dailyShareItemList.length; i++) {
    if (dailyShareItemList[i].id == id) {
      return dailyShareItemList[i]
    }
  }
  return dailyShareItemList[0];
}